<% user.forEach( u => { %>
  
     <nav aria-label="breadcrumb">
       <ol class="breadcrumb">
         <li class="breadcrumb-item"><a href="/www/test">Home</a></li>
         <li class="breadcrumb-item active" aria-current="page"><%- user.name %></li>
       </ol>
     </nav>
  
 
   <div class="row">
 
      <div class="col-12 col-md-4">
        <img src="/test/public/uploads/<%- user.image %>" class="img-fluid sticky-top" style="top: 20px;" alt="<%- user.name %>"> 
     </div>

      <div class="col-12 col-md-8">
        <div class="row">
          <div class="col-12"><h1><%- user.name %></h1></div>
          <div class="col-12 mb-4"><i class="bi bi-tag"></i> <%- user.category %></div>
          <div class="col-12"> Contact: <%- user.email %></div>
          <div class="col-12" style="white-space: pre-line;">
       </div>
 
      </div>
    </div>
<% }); %>
